{
  "expo": {
    "name": "NutriBreak",
    "slug": "nutribreak-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFB6C1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nutribreak.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFB6C1"
      },
      "package": "com.nutribreak.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "nutribreak"
  }
}
